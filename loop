package mat;
public class loop {

	public static void main(String[] args) {
		int i = 1,sum=0;
		while(i<=10){
			sum = sum+i;
			System.out.println("i = "+i +" Sum = "+sum); 
			i++;			
		}//end while
	}//end method
}//end class
/*
ผลการรันโปรแกรม
i = 1 Sum = 1
i = 2 Sum = 3
i = 3 Sum = 6
i = 4 Sum = 10
i = 5 Sum = 15
i = 6 Sum = 21
i = 7 Sum = 28
i = 8 Sum = 36
i = 9 Sum = 45
i = 10 Sum = 55
*/

